        "body": "\\begin{table}[${1:htbp}]\n\t\\centering\n\t\\import{${2:tablas/}}{${0:${TM_SELECTED_TEXT}}}\n\t\\caption{${3:<caption>}}\n\t\\label{${4:<label>}}\n\\end{table}",
        "description": "import table from external file"
    },

    "spanish": {
        "prefix": "spanish",
        "body": [
            "% Escritura de acentos:",
            "%--------------------------------------------------------------------------",
            "\\usepackage[utf8]{inputenc}",
            "\\usepackage[T1]{fontenc}\n",
            "% Selección de idioma (español)",
