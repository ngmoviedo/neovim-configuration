    "int": {
        "prefix": "int",
        "body": "\\int_{$1}^{$2}${0:${TM_SELECTED_TEXT}}",
        "description": "integral"
    },
    "iint": {
        "prefix": "iint",
        "body": "\\int_{-\\infty}^{+\\infty}${0:${TM_SELECTED_TEXT}}",
