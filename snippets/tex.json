            "\\usepackage{fancyhdr}\n",
            "\\pagestyle{fancy}",
            "\\fancyhf{}",
            "\\fancyfoot[LE,RO]{\\thepage}",
            "\\chead{\\leftmark}"
        ],
        "description": "header and foot for two-sided document"
    },
    "AMS": {
        "prefix": "AMS",
        "body": [
            "% Paquetes de la AMS",
            "%--------------------------------------------------------------------------",
            "\\usepackage{amsmath}",
            "\\usepackage{amsfonts}",
            "\\usepackage{amssymb}",
            "\\usepackage{amsthm}"
        ],
        "description": "AMS's packages"
    },
    "theorems_def": {
        "prefix": "thmdef",
        "body": [
            "% Teoremas",
            "%--------------------------------------------------------------------------",
            "\\newtheorem{thm}{Teorema}[section]",
            "\\newtheorem*{thm*}{Teorema}",
            "\\crefformat{thm}{teorema~(#2#1#3)}\n",
            "\\newtheorem{cor}{Corolario}[section]",
            "\\newtheorem*{cor*}{Corolario}",
            "\\crefformat{cor}{corolario~(#2#1#3)}\n",
            "\\newtheorem{lem}{Lema}[section]",
            "\\newtheorem*{lem*}{Lema}",
            "\\crefformat{lem}{lema~(#2#1#3)}\n",
            "\\newtheorem{prop}{Proposición}[section]",
            "\\newtheorem*{prop*}{Proposición}",
            "\\crefname{prop}{proposición}{proposiciones}",
            "\\crefformat{prop}{proposición~(#2#1#3)}\n",
            "\\theoremstyle{definition}",
            "\\newtheorem{defn}{Definición}[section]",
            "\\newtheorem*{defn*}{Definición}",
            "\\crefformat{defn}{definición~(#2#1#3)}\n",
            "\\theoremstyle{remark}",
            "\\newtheorem{obs}{Observación}[section]",
            "\\newtheorem*{obs*}{Observación}",
            "\\crefformat{obs}{observación~(#2#1#3)}",
            "\\newtheorem{ej}{Ejemplo}[section]",
            "\\newtheorem*{ej*}{Ejemplo}",
            "\\crefformat{ej}{ejemplo~(#2#1#3)}"
        ],
        "description": "theorem definitions"
    },
    "matlab_code": {
        "prefix": "mcode",
        "body": [
            "% Inserción de código",
            "%--------------------------------------------------------------------------",
            "\\usepackage[numbered,framed]{matlab-prettifier}",
            "\\lstset{literate=\n",
            "\t{á}{{\\'a}}1 {é}{{\\'e}}1 {í}{{\\'i}}1 {ó}{{\\'o}}1 {ú}{{\\'u}}1",
            "\t{Á}{{\\'A}}1 {É}{{\\'E}}1 {Í}{{\\'I}}1 {Ó}{{\\'O}}1 {Ú}{{\\'U}}1",
            "\t{à}{{\\`a}}1 {è}{{\\`e}}1 {ì}{{\\`i}}1 {ò}{{\\`o}}1 {ù}{{\\`u}}1",
            "\t{À}{{\\`A}}1 {È}{{\\'E}}1 {Ì}{{\\`I}}1 {Ò}{{\\`O}}1 {Ù}{{\\`U}}1",
            "\t{ä}{{\\\"a}}1 {ë}{{\\\"e}}1 {ï}{{\\\"i}}1 {ö}{{\\\"o}}1 {ü}{{\\\"u}}1",
            "\t{Ä}{{\\\"A}}1 {Ë}{{\\\"E}}1 {Ï}{{\\\"I}}1 {Ö}{{\\\"O}}1 {Ü}{{\\\"U}}1",
            "\t{â}{{\\^a}}1 {ê}{{\\^e}}1 {î}{{\\^i}}1 {ô}{{\\^o}}1 {û}{{\\^u}}1",
            "\t{Â}{{\\^A}}1 {Ê}{{\\^E}}1 {Î}{{\\^I}}1 {Ô}{{\\^O}}1 {Û}{{\\^U}}1",
            "\t{ã}{{\\~a}}1 {ẽ}{{\\~e}}1 {ĩ}{{\\~i}}1 {õ}{{\\~o}}1 {ũ}{{\\~u}}1",
            "\t{Ã}{{\\~A}}1 {Ẽ}{{\\~E}}1 {Ĩ}{{\\~I}}1 {Õ}{{\\~O}}1 {Ũ}{{\\~U}}1",
            "\t{œ}{{\\oe}}1 {Œ}{{\\OE}}1 {æ}{{\\ae}}1 {Æ}{{\\AE}}1 {ß}{{\\ss}}1",
            "\t{ű}{{\\H{u}}}1 {Ű}{{\\H{U}}}1 {ő}{{\\H{o}}}1 {Ő}{{\\H{O}}}1",
            "\t{ç}{{\\c c}}1 {Ç}{{\\c C}}1 {ø}{{\\o}}1 {å}{{\\r a}}1 {Å}{{\\r A}}1",
            "\t{€}{{\\euro}}1 {£}{{\\pounds}}1 {«}{{\\guillemotleft}}1",
            "\t{»}{{\\guillemotright}}1 {ñ}{{\\~n}}1 {Ñ}{{\\~N}}1 {¿}{{?`}}1 {¡}{{!`}}1",
            "}\n",
            "\\lstset{style=Matlab-editor,basicstyle=\\mlttfamily}"
        ],
        "description": "Matlab code insertion options"
    },
    "titlepage": {
        "prefix": "titlepage",
        "body": [
            "% Título, autor, materia y fecha (rellenar)",
            "%--------------------------------------------------------------------------\n",
            "\\newcommand{\\titulo}{${1:<titulo>}}",
            "\\newcommand{\\subtitulo}{${2:<subtitulo>}}\n",
            "\\newcommand{\\escudo}{${3:<escudo>}}\n",
            "\\newcommand{\\institucion}{${4:<institucion>}}\n",
            "\\newcommand{\\autor}{${5:<autor>}}\n",
            "\\newcommand{\\materia}{${6:<materia>}}\n",
            "\\newcommand{\\profesor}{${7:<profesor>}}\n",
            "\\newcommand{\\curso}{${8:<curso>}}\n",
            "\\newcommand{\\ciudad}{${9:<ciudad>}}\n",
            "\\newcommand{\\fecha}{${10:<fecha>}}\n",
            "% Portada",
            "%----------------------------------------------------------------------\n",
            "\\begin{titlepage}\n",
            "    \\centering\n",
            "    \\includegraphics[width=0.5\\textwidth]{\\escudo}\\par\\vspace{0.2cm}\n",
            "    {\\scshape\\LARGE \\institucion}\\par\n",
            "    \\vspace{2.5cm}\n",
            "    {\\huge\\bfseries \\titulo}\\par\n"	,
            "    {\\Large\\bfseries \\subtitulo}\\par\n",
            "    \\vspace{1.5cm}\n",
            "    {\\Large \\textit{\\materia}\\par (curso académico \\curso)}\\par\n",
            "    {\\Large\\itshape presentado por}\\par",
            "    \\vspace{.25cm}\n",
            "    {\\Large \\autor}\\par",
            "    \\vspace{1cm}\n",
            "    {\\Large  tutorado por}\\par",
            "    {\\Large \\scshape \\profesor}\\par",
            "    \\vfill\n",
            "    \\textsc{\\ciudad}\\par\n",
            "    \\fecha\n",
            "\\end{titlepage}\n",
            "% Dejamos la siguiente página en blanco y sin numerar:\n",
            "\\clearpage\\thispagestyle{empty}\\mbox{}\\setcounter{page}{0}\\clearpage\n$0"
        ],
        "description": "Spanish titlepage"
    }
}
