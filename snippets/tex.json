	"figure": {
		"prefix": "figure",
		"body": "\\begin{figure}[${1:htbp}]\n\t\\centering\n\t\\includegraphics[width=${2:\\textwidth}]{${0:${TM_SELECTED_TEXT}}}\n\t\\caption{${3:<caption>}}\n\t\\label{${4:<label>}}\n\\end{figure}",
